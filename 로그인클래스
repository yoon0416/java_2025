class Login1 {
    public boolean login(String[][] userData, String idInput, String pwInput) {
        boolean fine = false;
        for (int i = 0; i < userData.length; i++) {
            if (userData[i][0] != null && userData[i][0].equals(idInput) && userData[i][1].equals(pwInput)) {
                fine = true;
                break;
            }
        }
        return fine;
    } //end login 메소드
} //end login1 class
